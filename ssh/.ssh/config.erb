<% if osx? %>
Host *
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%r@%h:%p
  ControlPersist 10m
  UseKeychain yes
  AddKeysToAgent yes

Host *.anyone.no, *.elektronaut.no, elektronaut.no, *.b3s.me, b3s.me
  RemoteForward 9999 localhost:9999
  ForwardAgent yes
<% end %>

Host *.anyone.no
  User rails

Host *.b3s.me, b3s.me
  User app
