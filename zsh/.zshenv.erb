export GOPATH="$HOME/Dev/go"
export LC_CTYPE="en_US.UTF-8"
export PATH=$HOME/.bin:$HOME/.iterm2:$HOME/.rbenv/bin:$HOME/.rbenv/shims:/usr/local/opt/qt@5.5/bin:/usr/local/rbenv/bin:/opt/local/bin:/opt/local/sbin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/X11/bin:/usr/bin/X11:/usr/games:/usr/X11R6/bin:/usr/local/mysql/bin
export RIPGREP_CONFIG_PATH=~/.rgconfig
export GITHUB_TOKEN="<%= secret :github_token %>"
export MYSQL_REMOTE_USER="<%= secret(:mysql_remote_user) %>"
export MYSQL_REMOTE_PASSWORD="<%= secret(:mysql_remote_password) %>"
export DO_SPACES_ID="<%= secret(:do_spaces_id) %>"
export DO_SPACES_SECRET="<%= secret(:do_spaces_secret) %>"

<% if osx? %>
export SHELL_SESSIONS_DISABLE=1
eval "$(/opt/homebrew/bin/brew shellenv)
<% else %>
export RBENV_ROOT=/usr/local/rbenv
export PATH="$RBENV_ROOT/bin:$RBENV_ROOT/shims:$PATH"
<% end %>

<% if rbenv? %>
# Load rbenv
eval "$(rbenv init -)"
<% end %>
